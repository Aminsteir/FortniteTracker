<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kodoom.com.fortnitetrackernewrelease">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FortniteMainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".playerStatsActivity" android:screenOrientation="portrait"/>
        <activity android:name=".fortniteStoreActivity" />
        <activity android:name=".upcomingItemsActivity" />
        <activity android:name=".activity_location" android:screenOrientation="portrait"/>
        <activity android:name=".activity_player" android:screenOrientation="portrait"/>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <!--<activity android:name=".FortniteStoreAdapterActivity" />-->
        <!--<activity android:name=".fortnite_upcoming_adapter"></activity>-->

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1989206618089867~6787489885"/>

    </application>

</manifest>